{
  "environment_config": {
    "environment": "development",
    "debug": true,
    "hot_reload": true,
    "base_config": ${input.base_config}
  },
  "database": {
    "url": "${input.database_url}",
    "pool_size": 10,
    "max_connections": 20,
    "ssl_mode": "prefer",
    "log_statements": true
  },
  "storage": {
    "bucket": "${input.storage_bucket}",
    "region": "${var.aws_region}",
    "public_read": false,
    "versioning": true
  },
  "caching": {
    "enabled": true,
    "backend": "redis",
    "host": "localhost",
    "port": 6379,
    "db": 0,
    "ttl": 3600
  },
  "external_apis": {
    "rate_limit": 1000,
    "timeout": "60s",
    "retry_attempts": 5
  },
  "features": {
    "experimental_features": true,
    "debug_toolbar": true,
    "profiling": true,
    "mock_external_services": true
  },
  "notifications": {
    "email": {
      "provider": "mailhog",
      "host": "localhost",
      "port": 1025
    },
    "slack": {
      "enabled": false,
      "webhook_url": ""
    }
  },
  "monitoring": {
    "metrics_collection": true,
    "performance_monitoring": true,
    "error_tracking": true,
    "log_level": "debug"
  }
}